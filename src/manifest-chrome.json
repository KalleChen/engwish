{
  "name": "Engwish",
  "description": "Enter the Engwish World",
  "version": "1.0.0",
  "author": "Kalle Chen",
  "manifest_version": 3,
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "/img/skittlechan_16x16.png",
    "32": "/img/skittlechan_32x32.png",
    "48": "/img/skittlechan_48x48.png",
    "128": "/img/skittlechan_128x128.png"
  },
  "content_scripts": [
    {
      "js": [
        "/scripts/contentScript.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "action": {
    "default_popup": "/popup.html"
  }
}
